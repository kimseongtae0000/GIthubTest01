CREATE TABLE PERSON (
    ID      NUMBER(5),
    NAME    VARCHAR2(30)
);
ALTER TABLE PERSON
ADD HP VARCHAR2(20);
ALTER TABLE PERSON
RENAME COLUMN HP TO TEL;
ALTER TABLE PERSON
MODIFY TEL VARCHAR2(13);
ALTER TABLE PERSON
DROP COLUMN TEL;
SELECT * FROM PERSON;

--P349
CREATE TABLE DEPT_SEQUENCE
AS SELECT * FROM DEPT WHERE 1 <> 1;

SELECT * FROM DEPT_SEQUENCE;
--시퀀스 만들기
CREATE SEQUENCE SEQ_DEPT_SEQUENCE
INCREMENT BY 10
START WITH 10
MAXVALUE 90
MINVALUE 0
NOCYCLE
CACHE 2;
