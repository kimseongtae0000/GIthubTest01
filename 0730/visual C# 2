namespace BoxingApp
{
    internal class Program
    {
        static void Main(string[] args)
        {
            char ch1 = 'A';
            char ch2 = 'B';
            ch1 = ch2;
            ch2 = ch1;

            float f1 = 3.14F;
            Double f2 = f1;
            f1 = (float)f2;
        }
    }
}
