namespace LamdaTest01
{
    class Calculator
    {
        public int Plus(int a, int b)
        {
            return a + b;
        }
    }
    internal class Program
    {
        delegate int Calculate(int a, int b);
        static void Main(string[] args)
        {
            Calculate calc = (int a, int b) => a + b;

            Console.WriteLine(calc(10,20));
        }
    }
}

